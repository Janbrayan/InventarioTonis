  "build": {
    "appId": "com.janbrayan.inventariotonis",
    "productName": "InventarioTonis",
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Janbrayan",
        "repo": "InventarioTonis",
        "releaseType": "release"
      }
    ]
