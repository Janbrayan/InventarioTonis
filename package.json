    "@mui/system": "^6.4.2",
    "@mui/x-date-pickers": "^7.25.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.8.1",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.3.9",
    "framer-motion": "^11.18.1",
    "pdfkit": "^0.16.0",
    "react-router-dom": "^6.28.2",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.12",
